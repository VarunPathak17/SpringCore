<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  
  <bean id="student" class="com.java.di.Student" >
	<constructor-arg name="studentName" value="Varun" type="java.lang.String"/>
	<constructor-arg name="id" value="10" type="int"/>
  </bean>
  <bean id="dilip" class="com.java.di.Student" >
	<constructor-arg name="id" value="11"/>
	
  </bean>
   </beans>